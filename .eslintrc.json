{
  "extends": "next/core-web-vitals",
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "jsx-quotes": [2, "prefer-double"],
    "quotes": [2, "single"],
    "semi": 2,
    "indent": [2, 2],
    "prefer-destructuring": [2, {"object": true, "array": true}],
    "max-len": [
      "error",
      {
        "code": 80,
        "ignoreUrls": true,
        "ignoreHTMLTextContents": true
      }
    ]
  }
}
